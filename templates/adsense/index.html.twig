{% extends 'base.html.twig' %}

{% block title %}Hello AdsenseController!{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

<div class="example-wrapper">
    <h1>Hello</h1>

    {% for adsense in adsenses %}
        <p>
            {{adsense.id}}<br>
            {{adsense.title}}<br>
            <!--Ao clicar em acessar serÃ¡ redirecionado para a adsense por id -->
             <!--Use o path e pegue os nomes abaixo no aquivo AdsenseController-->
            <a href="{{path('adsenses_edit', {'id': adsense.id})}}">Acessar</a>
        </p>
    {% endfor %}    
</div>
{% endblock %}
